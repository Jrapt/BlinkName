        delay(500);
        digitalWrite(led1,LOW);
        delay(250);
        
        return 2;
    }
     else if (command=="C") {
          //_
        digitalWrite(led1,HIGH);
        delay(1000);
        digitalWrite(led1,LOW);
        delay(250);
         //.
