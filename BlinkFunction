//Start

int led1 = D7;


//pin input, output and function
void setup()
{

   pinMode(led1, OUTPUT);


   Particle.function("led",ledToggle);

   digitalWrite(led1, LOW);


}



void loop()
{
 
}


//functions if you type letter it will come out in morse code
//input is used from the Particle app
int ledToggle(String command) {


    if (command=="A") {
        //.
        digitalWrite(led1,HIGH);
        delay(500);
        digitalWrite(led1,LOW);
        delay(250);
        //_
        digitalWrite(led1,HIGH);
        delay(1000);
        digitalWrite(led1,LOW);
        delay(250);
        return 1;
    }
    else if (command=="B") {
        //_
        digitalWrite(led1,HIGH);
        delay(1000);
        digitalWrite(led1,LOW);
        delay(250);
        //.
        digitalWrite(led1,HIGH);
        delay(500);
        digitalWrite(led1,LOW);
        delay(250);
        //.
        digitalWrite(led1,HIGH);
        delay(500);
        digitalWrite(led1,LOW);
        delay(250);
        //.
